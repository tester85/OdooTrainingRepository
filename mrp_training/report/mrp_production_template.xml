<?xml version="1.0"?>
<odoo>
    <template id="internal_layout_inherit" inherit_id="web.internal_layout">
        <xpath expr="//div[@class='col-3']" position="replace">
            <div class="align-items-start">
            </div>
        </xpath>
        <!--        Eliminando otros valores de fecha Nombre-->
        <!--        Nombre-->
        <xpath expr="//div[@class='col-2 offset-3 text-right']" position="replace">
            <span t-esc="company.name"/>
        </xpath>
        <xpath expr="//div[@class='col-2 offset-2 text-center']" position="after">
            <div class="d-flex align-items-start flex-row">
                <br/>
                <strong>‘’REPORTE DE ENSAMBLE DE PRODUCTOS’’</strong>
                <br/>
            </div>
        </xpath>
    </template>
</odoo>